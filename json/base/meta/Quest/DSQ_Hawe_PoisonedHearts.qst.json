       "szOnCallbackCompleteScript": "function Start(idActor, idActorDetector)\r\n\r\n\t-- Get references\r\n\tidGulyas = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"Gulyas\")[1]\r\n\tidAnetaPlaceGoreLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"AnetaPlaceGoreLoc\")[1]\r\n\tidAnetaDrinkLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"AnetaDrinkLoc\")[1]\r\n\tidAnetaWaveEventLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"AnetaWaveEventLoc\")[1]\r\n\tidGulyasWaveEventLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"GulyasWaveEventLoc\")[1]\r\n\tidGulyasPostRiteLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"GulyasPostRiteLoc\")[1]\r\n\tidVass = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"Vass\")[1]\r\n\tidVera = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"Vera\")[1]\r\n\tidVassRunLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"VassRunLoc\")[1]\r\n\tidVeraRunLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"VeraRunLoc\")[1]\r\n\tidBloodRing = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"BloodRing\")[1]\r\n\tidBloodAltar = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"BloodAltar\")[1]\r\n\tidVassAttackLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"VassAttackLoc\")[1]\r\n\tidVeraFleeLoc = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"VeraFleeLoc\")[1]\r\n\tidVassCorpse = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"VassCorpse\")[1]\r\n\tidVassFlames = Hydra.ActorsLinkedByActorInGroup(idActorDetector, \"VassFlames\")[1]\r\n\ttVillagers = Hydra.ActorsInGroup(\"DSQ_Hawe_PoisonedHearts_VillagersAltar\")\r\n\ttVillagerLocs = Hydra.ActorsInGroup(\"DSQ_Hawe_PoisonedHearts_VillagersLocs\")\r\n\ttGore = Hydra.ActorsInGroup(\"DSQ_Hawe_PoisonedHearts_Gore\")\r\n\ttSpawners = Hydra.ActorsInGroup(\"DSQ_Hawe_PoisonedHearts_Spawners\")\r\n\t\r\n\tHydra.WaitForQEmpty(idAneta)\r\n\tHydra.ActorPlaySimpleConversation(convGulyasGreet, idGulyas)\r\n\tHydra.Wait(1.5)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idAneta, idAnetaPlaceGoreLoc, \"NPC_Walk_Fast\", true)\r\n\tHydra.QActorFaceSameAsActor(idAneta, idAnetaPlaceGoreLoc)\r\n\tHydra.QActorAnim(idAneta, \"AnimKey_ItemGive\")\r\n\t\r\n\tHydra.Wait(5)\r\n\tfor i, Gore in ipairs(tGore) do\r\n\t\tHydra.ActorEnable(Gore)\r\n\tend\r\n\t\r\n\tHydra.QActorFaceActor(idAneta, idGulyas)\r\n\t\r\nend\r\n",

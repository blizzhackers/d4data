       "szOnCallbackCompleteScript": "function Start(idActorChargeable)\r\n\r\n\tfor i, Spawner in ipairs(tSpawners) do\r\n\t\tHydra.ActorDisable(Spawner)\r\n\tend\r\n\t\r\n\ttBeasts = Hydra.ActorsInGroup(\"DSQ_Hawe_PoisonedHearts_Beasts\")\r\n\t\r\n\tif #tBeasts > 0 then\r\n\t\tfor i, Beast in ipairs(tBeasts) do\r\n\t\t\tHydra.ActorKill(Beast)\r\n\t\tend\r\n\tend\r\n\t\r\n\tHydra.Wait(1)\r\n\tHydra.ActorTriggerPhaseJump(idGulyas)\r\n\tHydra.Wait(0.35)\r\n\tHydra.ActorDisable(idBloodRing)\r\n\tHydra.ActorTriggerPhaseJump(idAneta)\r\n\tHydra.ActorRemoveComplexEffectByEffectGroupSNO(idAneta, \"fxKit_harmless_flameAura_sorcFire\")\r\n\tHydra.ActorRemoveComplexEffectByEffectGroupSNO(idGulyas, \"fxKit_damaging_persistentCast_2hand_blood\")\r\n\t\r\n\tHydra.ActorPlaySimpleConversation(convGulyasEndRitual, idGulyas, idAneta)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idGulyas, idGulyasPostRiteLoc, \"NPC_Walk\", true)\r\n\t\r\n\tHydra.QActorWait(idAneta, 0.5)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idAneta, idAnetaDrinkLoc, \"NPC_Run\", true)\r\n\tHydra.QActorFaceSameAsActor(idAneta, idAnetaDrinkLoc)\r\n\tHydra.QActorSetAnimSetOverride(idAneta, \"NPC_Standing_Drink\")\r\n\tHydra.WaitForQEmpty(idAneta)\r\n\tHydra.Wait(1)\r\n\r\n\t\r\n\tHydra.ActorEnable(idVass)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idVass, idVassRunLoc, \"NPC_Run\", true)\r\n\t\r\n\tHydra.ActorEnable(idVera)\r\n\tHydra.QActorWait(idVera, 0.35)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idVera, idVeraRunLoc, \"NPC_Run\", true)\r\n\tHydra.QActorFaceSameAsActor(idVera, idVeraRunLoc)\r\n\tHydra.QActorSetAnimSetOverride(idVera, \"NPC_Idle_Stand_Exhausted\")\r\n\tHydra.Wait(2)\r\n\tHydra.ActorPlaySimpleConversation(convVassArrives, idVass, idVera, idAneta, idGulyas)\r\n\tHydra.QActorFaceSameAsActor(idVass, idVassRunLoc)\r\n\tHydra.QActorWait(idVass, 25)\r\n\tHydra.QActorAnim(idVass, \"AnimKey_Point_Eyelevel\")\r\n\t\r\n\r\n\t\r\n\tfor i, Villager in ipairs(tVillagers) do\r\n\r\n\t\tHydra.ActorEnable(Villager)\r\n\t\tHydra.QActorMoveToLocationDynamicSpeed(Villager, tVillagerLocs[i], \"NPC_Run\", true)\r\n\t\tHydra.QActorFaceSameAsActor(Villager, tVillagerLocs[i])\r\n\t\r\n\tend\r\n\t\r\n\tHydra.QActorClearAnimSetOverride(idAneta)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idAneta, idAnetaPlaceGoreLoc, \"NPC_Run\", true)\r\n\tHydra.QActorFaceActor(idAneta, idVassRunLoc)\r\n\tHydra.QActorFaceActor(idGulyas, idVeraRunLoc)\r\n\t\r\n\tHydra.WaitForConversationFinished(convVassArrives)\r\n\t\r\n\tfor i, Villager in ipairs(tVillagers) do\r\n\t\r\n\t\tHydra.ActorSetTeam(Villager, Hydra.TEAM_MONSTER_DEFAULT)\r\n\t\r\n\tend\r\n\t\r\n\tHydra.ActorEnableAI(idGulyas)\r\n\tHydra.ActorSetTeam(idGulyas, Hydra.TEAM_PLAYER_DEFAULT)\r\n\tHydra.ActorSetSheathPreference(idGulyas, 2)\r\n\tHydra.QActorUnsheath(idGulyas)\r\n\tHydra.ActorSetTeam(idAneta, Hydra.TEAM_PLAYER_DEFAULT)\r\n\tHydra.QActorPhasedAnimLoop(idAneta, \"AnimKey_Cower\", 0)\r\n\tHydra.ActorAddAITarget(tVillagers[3], idAneta)\r\n\tHydra.ActorClearAnimSetOverride(idVera)\r\n\t\r\nend\r\n",

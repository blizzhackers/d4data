       "szOnCallbackCompleteScript": "function Start(idActorKiller, szGroup)\r\n\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idGulyas, idGulyasPostRiteLoc, \"NPC_Run\", true)\r\n\tHydra.QActorFaceActor(idGulyas, idVassCorpse)\r\n\t\r\n\tHydra.Wait(1)\r\n\tHydra.ActorSetPowerEffectSize(idVass, 5)\r\n\tHydra.ActorPlaySimpleConversation(convVassAttacks, idVass, idVera, idAneta)\r\n\tHydra.QActorFaceActor(idVass, idVera)\r\n\tHydra.QActorFaceActor(idVera, idVass)\r\n\tHydra.QActorFaceActor(idGulyas, idVera)\r\n\tHydra.Wait(1)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idVass, idVassAttackLoc, \"IGC_NPC_Walk_Slow\", true)\r\n\tHydra.QActorSetAnimSetOverride(idVass, \"NPC_Idle_Stand_Strong\")\r\n\tHydra.QActorWait(idVera, 0.2)\r\n\tHydra.QActorAnimLoop(idVera, \"AnimKey_Backpedal\", 4.5)\r\n\tHydra.QActorAnim(idVera, \"AnimKey_Talk_Scared\")\r\n\tHydra.ActorTriggerPhaseJump(idAneta)\r\n\tHydra.QActorFaceActor(idAneta, idVass)\r\n\tHydra.Wait(9)\r\n\tHydra.QActorFaceActor(idAneta, idVassCorpse)\r\n\tHydra.QActorPlayEffectGroup(idAneta, \"fxKit_damaging_persistentCast_2hand_fire\", true)\r\n\tHydra.QActorAnim(idAneta, \"AnimKey_Point_Eyelevel\")\r\n\tHydra.Wait(2)\r\n\tHydra.ActorRemoveComplexEffectByEffectGroupSNO(idAneta, \"fxKit_damaging_persistentCast_2hand_fire\")\r\n\tHydra.ActorPlayEffectGroup(idVass, \"fxKit_harmless_burst_ignite_fire\", false)\r\n\tHydra.ActorPlayEffectGroup(idVass, \"environmentVFX_fire_source_scalable_smokeless\", true, Hydra.ACDID_INVALID, \"HP_uniqueFX\")\r\n\tHydra.QActorSetAnimSetOverride(idVass, \"NPC_Idle_Stand_Burn\")\r\n\tHydra.Wait(3)\r\n\tHydra.QActorClearAnimSetOverride(idVass)\r\n\tHydra.QActorPhasedAnimLoop(idVass, \"AnimKey_Die_Phased\", 0)\r\n\tHydra.Wait(3)\r\n\tHydra.ActorFadeAndDelete(idVass, 0.5)\r\n\tHydra.ActorEnable(idVassCorpse)\r\n\tHydra.Wait(2.5)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idVera, idVeraFleeLoc, \"NPC_Run\", true)\r\n\tHydra.QActorFadeAndDelete(idVera, 0.5)\r\n\tHydra.Wait(2)\r\n\tHydra.QActorMoveToLocationDynamicSpeed(idAneta, idVeraFleeLoc, \"NPC_Run\", true)\r\n\tHydra.QActorFadeAndDelete(idAneta, 0.5)\r\n\r\nend\r\n",
